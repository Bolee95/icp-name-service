{
  "scripts": {
    "dfx_install": "DFX_VERSION=0.15.1 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
    "replica_start": "dfx start --background --clean",
    "replica_stop": "dfx stop",
    "create_identities": "dfx identity new owner && dfx identity new user",
    "canister_deploy_local": "dfx deploy icpns --identity owner",
    "canister_uninstall": "dfx canister uninstall-code icpns"
  },
  "dependencies": {
    "azle": "0.19.0"
  },
  "engines": {
    "node": "18"
  }
}
